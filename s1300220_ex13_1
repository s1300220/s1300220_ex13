import csv
import pandas as pd
from PAMI.extras.DF2DB import DenseFormatDF as db
df = pd.read_csv('air_pollution_data.csv')
df = df.fillna(0,inplace = True)
df = df.where(df <=100,0, inplace= True)
df.drop(culumns = ['timestanp'],inplace = True,axis = 1)
obj = db(df)
trasaction_obj = obj.convert2TransactionalDatabase(condiction = '>=',threshold = 15)
transaction_obj.to_csv('PM24HeavyPollutionRecordingSensors.csv',index = False)
